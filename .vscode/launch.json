{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "JHipster Mocha Tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--timeout",
                "999999",
                "--reporter",
                "spec",
                "${workspaceFolder}/test"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster generate dotnetcore blueprint",
            "program": "${workspaceFolder}/node_modules/generator-jhipster/cli/jhipster.js",
            "timeout": 100000,
            "args": [
                "-d",
                "--skip-checks",
                "--skip-git",
                "--skip-cache",
                "--skip-install",
                "--no-insight",
                "--blueprints",
                "dotnetcore"
                ],
            "cwd": "${workspaceFolder}/test-integration/samples/jwt-with-angular-app",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster entity",
            "program": "${workspaceFolder}/node_modules/generator-jhipster/cli/jhipster.js",
            "args": [
                "entity",
                "BankAccount",
                "--force"
            ],
            "cwd": "${workspaceFolder}/test-integration/samples/jwt-with-angular-app",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster heroku",
            "program": "${workspaceFolder}/node_modules/generator-jhipster/cli/jhipster.js",
            "args": [
                "heroku",
            ],
            "cwd": "${workspaceFolder}/../estoquenuvem/",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster info",
            "program": "${workspaceFolder}/node_modules/generator-jhipster/cli/jhipster.js",
            "args": [
                "info"
            ],
            "cwd": "${workspaceFolder}/test-integration/samples/jwt-with-angular-app",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster language",
            "program": "${workspaceFolder}/node_modules/generator-jhipster/cli/jhipster.js",
            "args": [
                "language"
            ],
            "cwd": "${workspaceFolder}/test-integration/samples/jwt-with-angular-app",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jhipster upgrade",
            "program": "${workspaceFolder}/node_modules/generator-jhipster/cli/jhipster.js",
            "args": [
                "upgrade"
            ],
            "cwd": "${workspaceFolder}/test-integration/samples/jwt-with-angular-app"
        }
    ]
}
